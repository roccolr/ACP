    "def reverse (mystr):\n",
    "    return mystr[::-1]\n",
    "\n",
    "def isPalindrome(s):\n",
    "    t = reverse(s)\n",
    "    if (s == t):\n",
    "        return True \n",
    "    else:\n",
    "        return False\n",
    "\n",
    "s = str(input(\"inserisci una stringa\"))\n",
    "if (isPalindrome(s)):\n",
    "    print(\"è palindroma!!!\")\n",
    "else:\n",
    "    print (\"non è palindroma!!\")"
   ]
  }
